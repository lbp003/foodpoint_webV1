{
    "name": "shanmuga/laravel-entrust",
    "description": "This package provides a flexible solution to add ACL to Laravel",
    "keywords": ["acl", "authorization", "php", "laravel","entrust", "roles", "permissions", "laravel-entrust"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shanmugarajan",
            "email": "shanmugarajan.33@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2.5",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Shanmuga\\LaravelEntrust\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shanmuga\\LaravelEntrust\\LaravelEntrustServiceProvider"
            ],
            "aliases": {
                "LaravelEntrust": "Shanmuga\\LaravelEntrust\\Facades\\LaravelEntrustFacade"
            }
        }
    }
}
